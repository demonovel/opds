{
  "version": 2,
  "builds": [
    {
      "src": "packages/web/package.json",
      "use": "@now/next@canary"
    }
  ],
  "routes": [

    {
      "src": "/api/(.*)",
      "headers": {
        "cache-control": "s-maxage=3600"
      },

      "continue": true
    },
    {
      "src": "(.*)",
      "dest": "packages/web$1",
      "continue": true
    }

  ],
  "github": {
    "silent": false,
    "autoJobCancelation": true
  }
}
