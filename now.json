{
  "version": 2,
  "builds": [
    {
      "src": "packages/web/package.json",
      "use": "@now/next@canary"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "packages/web$1",
      "continue": true
    }
  ],
  "github": {
    "silent": false,
    "autoJobCancelation": true
  }
}
