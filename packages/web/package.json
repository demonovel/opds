{
  "name": "demonovel",
  "version": "0.1.0",
  "private": true,
  "description": "This is a [Next.js](https://nextjs.org/) project bootstrapped with [`create-next-app`](https://github.com/zeit/next.js/tree/canary/packages/create-next-app).",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "engines": {
    "node": "12.x",
    "yarn": "1.22.0"
  },
  "scripts": {
    "prebuild": "node -v",
    "build": "esm-ts-node ./script/build && next build",
    "build:export": "yarn build && next export",
    "coverage": "npx nyc yarn run test",
    "dev": "next dev",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "npm:publish": "npm publish",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "sort-package-json": "npx yarn-tool sort",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.3",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.43",
    "@use-it/event-listener": "^0.1.3",
    "@zeit/next-sass": "^1.0.1",
    "build-json-cache": "1",
    "isomorphic-unfetch": "^3.0.0",
    "material-ui-theme-state": "^1.0.9",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "next": "9.2.2",
    "next-fonts": "^1.0.3",
    "node-sass": "npm:sass",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "^5.1.2",
    "react-swipeable": "^5.5.1",
    "react-use-gesture": "^7.0.4",
    "react-xml": "^0.3.0",
    "typescript": "^3.9.0-dev.20200221",
    "webpack-workspaces-support": "^1.0.5"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.18",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^13.7.4",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.22",
    "@types/react-dom": "^16.9.5",
    "esm": "^3.2.25",
    "esm-ts-node": "^1.0.5",
    "ts-node": "^8.6.2"
  }
}
