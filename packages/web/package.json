{
  "name": "demonovel",
  "version": "0.1.0",
  "private": true,
  "description": "This is a [Next.js](https://nextjs.org/) project bootstrapped with [`create-next-app`](https://github.com/zeit/next.js/tree/canary/packages/create-next-app).",
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "engines_": {
    "node": "12.x",
    "yarn": "1.22.0"
  },
  "scripts": {
    "prebuild": "node -v",
    "build": "esm-ts-node ./script/build && next build",
    "build:export": "yarn build && next export",
    "coverage": "npx nyc yarn run test",
    "dev": "next dev",
    "lint": "npx eslint **/*.ts",
    "ncu": "npx yarn-tool ncu -u",
    "npm:publish": "npm publish",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "prepublish:lockfile": "npx sync-lockfile .",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "sort-package-json": "npx yarn-tool sort",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:mocha": "npx mocha --require ts-node/register \"!(node_modules)/**/*.{test,spec}.{ts,tsx}\"",
    "tsc:default": "tsc -p tsconfig.json",
    "tsc:esm": "tsc -p tsconfig.esm.json"
  },
  "dependencies": {
    "@lazy-react/get-next-ctx": "^1.0.1",
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.53",
    "@use-it/event-listener": "^0.1.3",
    "@zeit/next-sass": "^1.0.1",
    "build-json-cache": "1",
    "is-req-https2": "^1.0.7",
    "isomorphic-unfetch": "^3.0.0",
    "jsx-linebreak": "^1.0.9",
    "lodash": "^4.17.15",
    "material-table": "^1.57.2",
    "material-ui-theme-state": "^1.0.17",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.31",
    "next": "9.4.2",
    "next-fonts": "^1.1.0",
    "node-sass": "npm:sass",
    "original-url2": "^1.2.10",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.2.0",
    "react-swipeable": "^5.5.1",
    "react-use-gesture": "^7.0.15",
    "react-use-localstorage2": "^4.0.8",
    "react-xml": "^0.3.0",
    "ts-type": "^1.2.16",
    "typescript": "^3.9.3",
    "webpack-workspaces-support": "^1.0.5"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.19",
    "@types/lodash": "^4.14.152",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^14.0.4",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "esm": "^3.2.25",
    "esm-ts-node": "^1.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^4.0.0-dev.20200521"
  }
}
